import static java.lang.System.*;

public class VowelCounter
{
    public static String getNumberString( String s)
    {
        String vow = "AEIOUaeiou";
        for(int i = 0; i < s.length();i++){
            if(vow.indexOf(i)>-1){
                return "";
            }
        }
        return "";
    }
}
